{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "ged-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/ged/GedWebPart.js",
          "manifest": "./src/webparts/ged/GedWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://code.jquery.com/jquery-3.1.1.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/3.3.7/js/bootstrap.min.js",
      "globalName": "bootstrap",
      "globalDependencies": [
        "jquery"
      ]
    }
  },
  "localizedResources": {
    "GedWebPartStrings": "lib/webparts/ged/loc/{locale}.js"
  }
}
